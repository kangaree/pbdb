<div id="<%= dom_id ticket %>">
  <p>
    <strong>Title:</strong>
    <%= ticket.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= ticket.content %>
  </p>

  <p>
    <strong>Ticket type:</strong>
    <%= ticket.ticket_type %>
  </p>

  <p>
    <strong>User name:</strong>
    <%= ticket.user_name %>
  </p>

  <p>
    <strong>User image:</strong>
    <%= ticket.user_image %>
  </p>

</div>
